expression =
  constant
  | parameter
  | operation
  | built-in
  | random-deviate
  | test-event

operation =
  numerical-operation | Boolean-operation | conditional-operation

built-in = exponential | GLM | Weibull | periodic-test | extern-function

random-deviate =
  uniform-deviate
  | normal-deviate
  | lognormal-deviate
  | gamma-deviate
  | beta-deviate
  | histogram
  | discrete

test-event = test-initiating-event | test-functional-event
